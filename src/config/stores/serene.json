        {
          "type": "aboutSplit",
          "enabled": true,
          "image": {
            "src": "https://centa-hr.s3.amazonaws.com/019b40f4-a8f1-7b26-84d0-45069767fa8c/IMG_5127.webp",
            "alt": "Serene Hospitality Solutions partnership"
          },
          "content": {
            "title": "Working Closely With You, Every Step of the Way",
